<form id="form" method="get" action="/">
  <table>
  <tr>
  <td><label>number</label><input type="text" name="number" value="<%= query.number %>">
  <td><label>mail</label><input type="text" name="mail" value="<%= query.mail %>">
  <td><label>name</label><input type="text" name="name" value="<%= query.name %>">
  </tr>
  <tr>
  <td><label>date</label><input type="text" name="date" value="<%= query.date %>">
  <td><label>id</label><input type="text" name="id" value="<%= query.id %>">
  <td><label>body</label><input type="text" name="body" value="<%= query.body %>">
  </tr>
  </table>
  <button type="reset" class="btn">リセット</button>
  <button class="btn" type="submit">検索</button>
</form>
<hr>
